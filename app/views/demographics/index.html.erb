

<h1>Demographics</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demographics.each do |demographic| %>
      <tr>
        <td><%= demographic.name %></td>
        <td><%= demographic.description %></td>
        <td><%= link_to 'Show', demographic %></td>
        <td><%= link_to 'Edit', edit_demographic_path(demographic) %></td>
        <td><%= link_to 'Destroy', demographic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demographic', new_demographic_path %>
