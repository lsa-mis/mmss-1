<p id="notice"><%= notice %></p>

<h1>Session Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Enrollment</th>
      <th>Camp occurrence</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @session_assignments.each do |session_assignment| %>
      <tr>
        <td><%= session_assignment.enrollment_id %></td>
        <td><%= session_assignment.camp_occurrence_id %></td>
        <td><%= link_to 'Show', session_assignment %></td>
        <td><%= link_to 'Edit', edit_session_assignment_path(session_assignment) %></td>
        <td><%= link_to 'Destroy', session_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session Assignment', new_session_assignment_path %>
