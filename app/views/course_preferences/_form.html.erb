<%= form_with(model: course_preference, local: true) do |form| %>
<h2 class="font-bold"><%= @course_preference.course.title %></h2>

  <div class="field">
    <%= form.label :ranking %>
    <%# <%= form.select :ranking, ((1..10).map {|i| [i,i] }), include_blank: true %>
     <%= form.select :ranking, (@remaining_selections.map {|i| [i,i] }), include_blank: true %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
