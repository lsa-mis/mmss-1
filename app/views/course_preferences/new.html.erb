<% if @current_enrollment_session2_courses %>
  <%# @current_enrollment_session2_courses.each do |cp| %>
    <%= form_with(model: @course_preference, local: true) do |cp_form| %>
      <% @current_enrollment_session2_courses.each do |cp| %>
