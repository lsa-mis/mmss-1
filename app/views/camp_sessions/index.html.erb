<p id="notice"><%= notice %></p>

<h1>Camp Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Camp configuration</th>
      <th>Description</th>
      <th>Begin date</th>
      <th>End date</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @camp_sessions.each do |camp_session| %>
      <tr>
        <td><%= camp_session.camp_configuration_id %></td>
        <td><%= camp_session.description %></td>
        <td><%= camp_session.begin_date %></td>
        <td><%= camp_session.end_date %></td>
        <td><%= camp_session.active %></td>
        <td><%= link_to 'Show', camp_session %></td>
        <td><%= link_to 'Edit', edit_camp_session_path(camp_session) %></td>
        <td><%= link_to 'Destroy', camp_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Camp Session', new_camp_session_path %>
